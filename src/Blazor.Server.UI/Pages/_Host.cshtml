@page "/"
@using Blazor.Server.UI
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
}

<component type="typeof(App)" render-mode="Server" />
<script>
    window.getClientTimeZone = function() {
        return Intl.DateTimeFormat().resolvedOptions().timeZone;
    }
</script>